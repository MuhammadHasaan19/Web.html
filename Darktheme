<!-- Dark Mode Toggle Button -->
<li class="tnb-dark-mode-toggle-wrapper" class="w3-bar-item">
  <a class="tnb-dark-mode-toggle-btn" href="javascript:void(0);" onclick="toggleDarkMode()">
    <i class="tnb-button fa ga-nav ga-dark-mode-toggle" role="button" title="Toggle light/dark mode" aria-label="Toggle light/dark mode">ðŸŒ™</i>
  </a>
</li>

<script>
  // Function to toggle dark mode
  function toggleDarkMode() {
    const body = document.body;
    body.classList.toggle('dark-mode'); // Toggle the 'dark-mode' class on the body
  }
</script>

</body>
</html>


